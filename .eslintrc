{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "no-shadow": "warn",
    "no-underscore-dangle": "off",
    "no-param-reassign": "off",
    "func-names": "off",
    "consistent-return": "off",
    "security/detect-object-injection": "off",
    "import/prefer-default-export": "off"
  }
}
